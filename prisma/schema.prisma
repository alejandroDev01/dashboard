generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model registro_votos {
  id               String  @id @default(cuid())
  timestamp        String?
  ip_publica       String?
  id_public_user   String?
  numero           String?
  latitud          String?
  longitud         String?
  ci               String?
  dia_nacimiento   String?
  mes_nacimiento   String?
  anio_nacimiento  String?
  pais             String?
  ciudad           String?
  candidato        String?
  tipo             tipo
  token_solicutd   String?
  codigo_respuesta String?
  respuesta        String?
  id_dispositivo   String?
  metodo           metodo
  refer_envio      String?
  token_refer      String?
}

enum metodo {
  TUXLER
  MODO_AVION
}

enum tipo {
  PRE_ENVIO
  VOTO_TOKEN
  VOTO_GERACION_TOKEN_ALEATORIA
  SIMULACION
}
