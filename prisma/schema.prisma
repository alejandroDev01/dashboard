generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Proxy {
  id          String        @id @default(cuid())
  host        String
  port        Int
  protocol    String
  createdAt   DateTime      @default(now())
  Solicitudes Solicitudes[]
}

model Solicitudes {
  id        String   @id @default(cuid())
  ipPublica String?
  status    String
  header    String?
  body      Json?
  method    String
  createdAt DateTime @default(now())
  proxyId   String
  proxy     Proxy    @relation(fields: [proxyId], references: [id])
}

// ale 
model PreEnvio {
  id             String  @id @default(cuid())
  timestamp      String?
  ip_actual      String?
  latitud        String?
  longitud       String?
  pais           String?
  ciudad         String?
  candidato      String?
  id_public_user String?
}

model Validado {
  id               String  @id @default(cuid())
  timestamp        String?
  ip_publica       String?
  id_public_user   String?
  latitud          String?
  longitud         String?
  pais             String?
  ciudad           String?
  candidato        String?
  codigo_respuesta String?
  texto_respuesta  String?
}

model Simulacion {
  id             String  @id @default(cuid())
  latitud        String?
  longitud       String?
  pais           String?
  ciudad         String?
  candidato      String?
  id_public_user String?
}
